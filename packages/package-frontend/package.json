    "@cxcloud/ct-types": "^0.1.6",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "karma-cli": "^1.0.1",
    "mkdirp": "^0.5.1",
    "ngx-markdown": "^8.1.0",
    "protractor": "^5.4.1",
    "rxjs-tslint": "^0.1.5",
    "serve": "^11.1.0",
